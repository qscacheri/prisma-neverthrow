{
    "name": "prisma-neverthrow",
    "description": "A Prisma generator that uses neverthrow to return errors instead of throwing them",
    "version": "0.1.0",
    "main": "dist/generator.js",
    "types": "dist/generator.d.ts",
    "license": "MIT",
    "prepublish": "tsc",
    "bin": {
        "prisma-generator-neverthrow": "dist/bin.js"
    },
    "engines": {
        "node": ">=14.0"
    },
    "scripts": {
        "start": "node dist/bin.js",
        "dev": "npx tsc -w",
        "build": "npx tsc",
        "prepack": "yarn build",
        "test": "jest"
    },
    "dependencies": {
        "@prisma/client": "3.12.0",
        "@prisma/generator-helper": "3.12.0",
        "@prisma/sdk": "3.12.0",
        "prettier": "2.5.1"
    },
    "devDependencies": {
        "@types/jest": "27.0.3",
        "@types/node": "17.0.21",
        "@types/prettier": "2.4.2",
        "jest": "27.4.7",
        "prisma": "3.12.0",
        "ts-jest": "27.1.4",
        "ts-morph": "^18.0.0",
        "typescript": "4.6.2"
    },
    "homepage": "https://github.com/qscacheri/prisma-neverthrow",
    "repository": {
        "type": "git",
        "url": "https://github.com/qscacheri/prisma-neverthrow"
    },
    "author": "Quin Scacheri",
    "keywords": [
        "prisma",
        "prisma2",
        "generator"
    ]
}
